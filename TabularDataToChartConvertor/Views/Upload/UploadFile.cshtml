
@{
    ViewBag.Title = "Chart Plotter";
}

<h2>UploadFile</h2>

@using (Html.BeginForm("GeneratePieChart", "ChartGenerator", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div>
        @Html.TextBox("file", "", new { type = "file" }) <br />
        <input type="submit" value="Upload" />
    </div>
}



